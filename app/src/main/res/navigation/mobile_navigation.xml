<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_inicio">

    <fragment
        android:id="@+id/nav_inicio"
        android:name="com.example.inmobiliariaderamo.ui.inicio.InicioFragment"
        android:label="@string/inicio"
        tools:layout="@layout/fragment_inicio" />

    <fragment
        android:id="@+id/nav_perfil"
        android:name="com.example.inmobiliariaderamo.ui.perfil.PerfilFragment"
        android:label="@string/perfil"
        tools:layout="@layout/fragment_perfil" />

    <fragment
        android:id="@+id/nav_inmuebles"
        android:name="com.example.inmobiliariaderamo.ui.Listar.ListarFragment"
        android:label="@string/inmuebles"
        tools:layout="@layout/fragment_slideshow" />
    <activity
        android:id="@+id/loginActivity"
        android:name="com.example.inmobiliariaderamo.ui.login.LoginActivity"
        android:label="activity_login"
        tools:layout="@layout/activity_login" />
    <fragment
        android:id="@+id/cambioContraFragment"
        android:name="com.example.inmobiliariaderamo.ui.CambioContraFragment"
        android:label="fragment_cambio_contra"
        tools:layout="@layout/fragment_cambio_contra" />
    <activity
        android:id="@+id/resetPasswordActivity"
        android:name="com.example.inmobiliariaderamo.ui.login.ResetPasswordActivity"
        android:label="ResetPasswordActivity" />
</navigation>